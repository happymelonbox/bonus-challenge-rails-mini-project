<%= form_for [@list, @item] do |f| %>

  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>

 <p>
   <%= f.label :quantity %><br>
   <%= f.number_field :quantity %>
 </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>